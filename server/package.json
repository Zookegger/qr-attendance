{
	"name": "qr_attendance_server",
	"version": "1.0.0",
	"description": "Backend for QR Attendance App",
	"license": "ISC",
	"author": "",
	"main": "dist/app.js",
	"scripts": {
		"test": "NODE_ENV=test mocha --import=tsx test/**/*.test.js",
		"start": "node dist/app.js",
		"dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
		"db:migrate": "npx sequelize-cli db:migrate",
		"db:migrate:undo": "npx sequelize-cli db:migrate:undo",
		"db:seed": "npx sequelize-cli db:seed:all",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:verbose": "jest --verbose",
		"build": "tsc && tsc-alias"
	},
	"dependencies": {
		"bcryptjs": "^3.0.3",
		"cors": "^2.8.5",
		"date-fns": "^4.1.0",
		"dotenv": "^16.3.1",
		"exceljs": "^4.4.0",
		"express": "^4.18.2",
		"firebase-admin": "^13.6.0",
		"ioredis": "^5.3.2",
		"jsonwebtoken": "^9.0.3",
		"mysql2": "^3.6.1",
		"node-cron": "^4.2.1",
		"passport": "^0.7.0",
		"sequelize": "^6.33.0",
		"socket.io": "^4.8.1",
		"sqlite3": "^5.1.7"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.14",
		"@types/express": "^4.17.18",
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/mocha": "^10.0.10",
		"@types/node": "^20.19.27",
		"@types/node-cron": "^3.0.11",
		"@types/passport": "^1.0.17",
		"@types/sequelize": "^4.28.16",
		"@types/supertest": "^6.0.3",
		"@types/uuid": "^10.0.0",
		"mocha": "^10.2.0",
		"nodemon": "^3.0.1",
		"sequelize-cli": "^6.6.1",
		"supertest": "^6.3.3",
		"ts-node": "^10.9.1",
		"tsc-alias": "^1.8.16",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.21.0",
		"typescript": "^5.2.2"
	}
}
